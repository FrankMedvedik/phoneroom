<UserControl x:Class="PhoneLogic.CallbackHistory.MainPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ignore="http://www.galasoft.ch/ignore"
             xmlns:views="clr-namespace:PhoneLogic.Core.Views;assembly=PhoneLogic.Core"
             xmlns:reportCriteria="clr-namespace:PhoneLogic.Core.Areas.ReportCriteria;assembly=PhoneLogic.Core"
             xmlns:callbacks="clr-namespace:PhoneLogic.Core.Areas.Callbacks;assembly=PhoneLogic.Core"
             mc:Ignorable="d ignore" Width="Auto">
    <Grid x:Name="LayoutRoot">
        <StackPanel Orientation="Vertical" Margin="10">
                <reportCriteria:PhoneroomsView  HorizontalAlignment="Left"/>
            <reportCriteria:GlobalReportCriteriaView HorizontalAlignment="Left" />
            <callbacks:ClosedCallbacksJobsView HorizontalAlignment="Left"/>
            <views:BuildInformationView />
        </StackPanel>
    </Grid>

</UserControl>
